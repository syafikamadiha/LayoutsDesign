    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="LayoutsDesign"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivityFrame">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivityAbsolute" />
        <activity android:name=".MainActivityTable">

        </activity>
        <activity android:name=".MainActivityRelative" />
        <activity android:name=".MainActivityLinear" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>